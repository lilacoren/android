<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />


    <uses-permission android:name="android.permission.NFC" />

    <uses-feature
        android:name="android.hardware.nfc"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.project2.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.project2.DisplayIntentTest"
            android:label="@string/title_activity_display_intent_test"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.project2.ActivitiesMenu"
            android:label="@string/title_activity_activities_menu"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.project2.SwipeScreen"
            android:label="@string/title_activity_swipe_screen"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.project2.SignIn"
            android:label="@string/title_activity_sign_in"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.project2.HandleCodeRead"
            android:label="@string/title_activity_handle_code_read"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.project2.History"
            android:label="@string/title_activity_history"
            android:screenOrientation="portrait" >
        </activity>

    </application>
        <uses-permission android:name="android.permission.INTERNET" />

</manifest>